{% spaceless %}
<footer class="main-footer">
    <div class="pull-right">
        <b>Powered by: <i class="fa fa-github"></i> <a href="https://github.com/SymfonyId/AdminBundle" target="_blank">AdminBundle</a> <i class="fa fa-github"></i></b>
    </div>
    <strong>Copyright &copy; 2015 {{ title }}.</strong> All rights reserved.
</footer>

{%- javascripts
    '@SymfonianIndonesiaAdminBundle/Resources/public/js/jquery-2.1.4.min.js'
    '@SymfonianIndonesiaAdminBundle/Resources/public/js/bootstrap.min.js'
    '@SymfonianIndonesiaAdminBundle/Resources/public/js/fastclick.min.js'
    '@SymfonianIndonesiaAdminBundle/Resources/public/js/jquery.slimscroll.min.js'
    '@SymfonianIndonesiaAdminBundle/Resources/public/js/app.min.js'
    'bundles/fosjsrouting/js/router.js'
    'js/fos_js_routes.js'

    filter='uglifyjs2'
-%}
    <script src="{{- asset_url -}}" type="text/javascript"></script>
{%- endjavascripts -%}

{%- if use_date_picker is sameas(true) -%}
    {%- javascripts
        '@SymfonianIndonesiaAdminBundle/Resources/public/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js'
        '@SymfonianIndonesiaAdminBundle/Resources/public/plugins/bootstrap-datepicker/locales/bootstrap-datepicker.id.min.js'

        filter='uglifyjs2'
    -%}
        <script src="{{- asset_url -}}" type="text/javascript"></script>
    {%- endjavascripts -%}
{%- endif -%}

{%- if use_file_style is sameas(true) -%}
    <script src="{{- asset('bundles/symfonianindonesiaadmin/plugins/bootstrap-filestyle/bootstrap-filestyle.min.js') -}}" type="text/javascript"></script>
{%- endif -%}

{%- if use_editor is sameas(true) -%}
    <script src="{{- asset('bundles/symfonianindonesiaadmin/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') -}}" type="text/javascript"></script>
{%- endif -%}

{%- if use_autocomplete is sameas(true) -%}
    <script src="{{- asset('bundles/symfonianindonesiaadmin/plugins/select2/js/select2.full.min.js') -}}" type="text/javascript"></script>
{%- endif -%}

{%- if use_numeric is sameas(true) -%}
    <script src="{{- asset('bundles/symfonianindonesiaadmin/plugins/select2/js/jquery.numeric.min.js') -}}" type="text/javascript"></script>
{%- endif -%}

{%- if javascript is not empty -%}
    {{- javascript | raw -}}
{%- endif -%}

<!--[if IE 8]>

<h1>Your browser is out of date, please update your browser by going to www.microsoft.com/download</h1>

<![endif]-->
{% endspaceless %}
