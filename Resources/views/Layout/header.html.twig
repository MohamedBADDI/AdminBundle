<meta charset="UTF-8">
<title>

{%- if page_title is not empty -%}
    {{- page_title | raw -}}
{%- else -%}
    {{- title | raw -}}
{%- endif -%}

</title>
<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

{%- stylesheets
    'bundles/symfonianindonesiaadmin/css/bootstrap.min.css'
    'bundles/symfonianindonesiaadmin/css/AdminLTE.min.css'
    'bundles/symfonianindonesiaadmin/css/skins/skin-green.min.css'
    'bundles/symfonianindonesiaadmin/css/style.css'

    filter='uglifycss, cssrewrite'
-%}
    <link rel="stylesheet" href="{{- asset_url -}}" />
{%- endstylesheets -%}

{%- if use_date_picker is not empty -%}
    <link rel="stylesheet" href="{{- asset('bundles/symfonianindonesiaadmin/plugins/bootstrap-datepicker/css/bootstrap-datepicker3.min.css') -}}" />
{%- endif -%}

{%- if use_editor is not empty -%}
    <link rel="stylesheet" href="{{- asset('bundles/symfonianindonesiaadmin/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') -}}" />
{%- endif -%}

{%- if css is not empty -%}
    {{- css | raw -}}
{%- endif -%}

<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet" type="text/css" />
